num1=int(input())
num2=int(input())
operator=input()
if operator=='+':
    print("sum of",num1,"and",num2,"is:",num1+num2)
elif operator=='-':
    print("Differnce of",num1,"and",num2,"is:",num1-num2)
elif operator=='*':
    print("product of",num1,"and",num2,"is:",num1*num2)
elif operator=='/':
    print("Quotient of",num1,"and",num2,"is:",num1/num2)
else:
    print("Invalid operator")
